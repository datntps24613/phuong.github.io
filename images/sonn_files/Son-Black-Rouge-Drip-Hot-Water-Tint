    // Image hover
    // imageZoom("myimage1", "myresult");
    function hoverImage(im)
    {
        var img = "myimage"+im;    
        $('.product-photos:not(.product-version-hide) .slider-nav .slick-slide .img-zoom-lens').remove();
        imageZoom(img, "myresult");
    }
  
</script>

<script>

    function checkPrograms(byuser) {
        var x = document.getElementById("programs").value;
        if(x>byuser && byuser !=0){
            alert("Bạn chỉ được mua " + byuser +" sản phẩm trong chương trình này")
            document.getElementById("programs").value = 1    ;
        }
    }
    function click() {
            var target = document.getElementById('closeLightBox');
            document.addEventListener('click', function (e) {
            target.style.left = 20 + 'px';
        });
    }
</script>        </section>
